		"cross-env": "^5.2.0",
		"express": "^4.16.4",
		"luxon": "^1.16.0",
		"moment": "^2.24.0",
		"nuxt": "^2.0.0",
		"vue-datetime": "^1.0.0-beta.10",
		"vue-js-modal": "^1.3.31",
		"vue-notification": "^1.3.16",
		"vuejs-paginate": "^2.1.0",
		"weekstart": "^1.0.0"
	},
	"devDependencies": {
		"node-sass": "^7.0.0",
		"nodemon": "^1.18.9",
		"sass-loader": "^7.1.0"
